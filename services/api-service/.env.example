# Server Configuration
PORT=3000
NODE_ENV=development

# Service URLs
DATA_SERVICE_URL=http://data-service-real:3002
SIGNAL_SERVICE_URL=http://signal-service:3002

# MongoDB Configuration
MONGODB_HOST=mongodb
MONGODB_PORT=27017
MONGODB_USER=liqpro
MONGODB_PASS=liqpro
MONGODB_DB=liqpro
MONGODB_URI=mongodb://${MONGODB_USER}:${MONGODB_PASS}@${MONGODB_HOST}:${MONGODB_PORT}/${MONGODB_DB}

# WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30000
WS_RECONNECT_INTERVAL=5000

# Service Health Check
HEALTH_CHECK_INTERVAL=30000

# Cache Configuration
CACHE_TTL=300000

# Logging Configuration
LOG_LEVEL=info 