# 应用配置
NODE_ENV=development
PORT=3000

# 安全配置
CORS_ORIGIN=http://localhost:3000
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=24h

# 日志配置
LOG_LEVEL=debug
LOG_DIR=logs

# Meteora配置
METEORA_API_KEY=your-meteora-api-key
METEORA_API_URL=https://api.meteora.ag/v1

# Jupiter配置
JUPITER_API_KEY=your-jupiter-api-key
JUPITER_API_URL=https://quote-api.jup.ag/v6

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=liqpro
DB_USER=postgres
DB_PASSWORD=your-db-password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# 监控配置
ENABLE_METRICS=true
METRICS_PORT=9090 